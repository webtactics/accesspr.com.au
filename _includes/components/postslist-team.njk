<div class="grid-2-columns-team" >
	{% for post in postslist | reverse %}
	<summary class="NewsSummary">
			
	

<div class="Postimagehover">
    <a href="{{ post.url | url }}"><img src="https://ik.imagekit.io/webtactics/accesspr/{{ post.data.image }}/tr:w-400,h-500" alt="{{ post.data.title }}" title="{{ post.data.title }}" width="450px" height="270px" class="postlistimg">
    </a>
</div>

<div class="teamcontentwrapper">
<div class="postsumtitle">
<h3 class="sum-title"><a href="{{ post.url | url }}" title="{{ post.data.title }}">{{ post.data.title }}</a></h3>
</div>


<div class="postsumsummary">
{% if post.data.description %}<p class="postsumhome">{{ post.data.description }}</p>{% else %}<p class="postsumhome">{{ post.content | safe }}</p>{% endif %}
</div>

</div>



		</summary>
		
	{% endfor %}
</div>


