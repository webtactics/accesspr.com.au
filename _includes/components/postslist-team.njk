<div class="grid-2-columns-team" >
	{% for post in postslist | reverse %}

			
	

<div class="teamimgwrapper">
    <a href="{{ post.url | url }}"><img src="https://ik.imagekit.io/webtactics/accesspr{{ post.data.image }}/tr:w-400,h-500" alt="{{ post.data.title }}" title="{{ post.data.title }}" width="200px" height="auto" class="teamlistimg">
    </a>
</div>

<div class="teamcontentwrapper">
<div class="postsumtitle">
<h3 class="team-sum-title"><a href="{{ post.url | url }}" title="{{ post.data.title }}">{{ post.data.title }}</a></h3>
</div>


<div class="teamsumsummary">
{% if post.data.description %}<p class="postsumhome">{{ post.data.description }}</p>{% else %}<div class="postsumhome">{{ post.content | safe }}</div>{% endif %}
</div>

</div>




		
	{% endfor %}
</div>


